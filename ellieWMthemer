
ellieThemer(){
    oldDir="$(pwd)"
    dir="/home/ellie/lnxpcsThemes/"
    cd $dir
    file=$(ls ~/lnxpcsThemes | shuf -n 1)
    echo $dir$file
    case "$file" in 
        "hello-linux-1280x800.png")
            wal -i $dir$file -l
            ;;
        "crossline-dots.png")
            wal -i $dir$file && feh --bg-tile $dir$file
            ;;
        *)
            wal -i $dir$file
            ;;
    esac
    cd $oldDir
}
